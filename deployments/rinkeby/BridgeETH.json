ount,\\n            block.timestamp,\\n            otherChainNonce,\\n            Step.Mint\\n        );\\n    }\\n\\n    modifier onlyOwner() {\\n        require(msg.sender == owner, \\\"ONLY_OWNER\\\");\\n        _;\\n    }\\n}\\n\",\"keccak256\":\"0xedcb4b9e4dbb8e338a673bfa626f6f935af9b00dbb65e129e31a8e1c00ffc5d3\"},\"contracts/BridgeETH.sol\":{\"content\":\"pragma solidity >=0.6.0 <0.8.0;\\n\\nimport \\\"./BridgeBase.sol\\\";\\n\\ncontract BridgeETH is BridgeBase {\\n    constructor(address token) BridgeBase(token) {}\\n}\\n\",\"keccak256\":\"0x398cb947db840609d611c81c04d88bc742dcf86fe269da97c9f0e4c1295f9e19\"},\"contracts/IToken.sol\":{\"content\":\"pragma solidity >=0.6.0 <0.8.0;\\n\\ninterface IToken {\\n    function mint(address account, uint256 amount) external;\\n\\n    function burn(address account, uint256 amount) external;\\n}\\n\",\"keccak256\":\"0xa51eb39fe241ee75295592ec2529840fbe72f94efb013a2e56b01d19e07070de\"}},\"version\":1}",
